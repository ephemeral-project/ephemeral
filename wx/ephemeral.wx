      !TopLeft(0, -20)
      !BottomRight
    OnLoad:
      *sn.tabbedframe(self, {
      *  {label='Summary', client='Summary'},
      *  {label='Modules', client='Modules'},
      *  {label='Settings', client='Settings'}
      *})
